{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single", "avoid-escape"],
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ],
    "max-line-length": [true, 250],
    "one-line": [false],
    "no-empty": false,
    "ordered-imports": [false],
    "member-ordering": [
      true,
      {
        "order": ["public-static-field", "protected-static-field", "private-static-field", "public-instance-field", "protected-instance-field", "private-instance-field", "constructor"]
      }
    ],
    "rulesDirectory": []
  }
}
